<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearHomeScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_901">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearNavbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_901">
            <ImageButton
                android:background="@drawable/rectangle_bg_gray_901_radius_24"
                android:id="@+id/btnUser" android:layout_gravity="end"
                android:layout_height="@dimen/_48pxh"
                android:layout_marginBottom="@dimen/_6pxv"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="@dimen/_48pxh"
                android:paddingHorizontal="@dimen/_13pxh"
                android:paddingVertical="@dimen/_13pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_user" tools:ignore="ContentDescription"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameStackcalendar"
            android:layout_height="@dimen/_580pxv" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearBody"
                android:layout_gravity="top|start"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_901">

                <LinearLayout
                    android:id="@+id/linearTileAppointments"
                    style="@style/groupStylegray_901"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageCalendar"
                        android:layout_width="@dimen/_18pxh"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_calendar"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_calendar" />

                    <TextView
                        android:id="@+id/txtAppointments"
                        style="@style/txtInterregular16_1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_marginEnd="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_weight="0.85"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_appointments"
                        tools:text="@string/lbl_appointments" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearTileClinics"
                    style="@style/groupStylegray_901"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageDomain"
                        android:layout_width="@dimen/_18pxh"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_domain"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_domain" />

                    <TextView
                        android:id="@+id/txtClinics"
                        style="@style/txtInterregular16_1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_marginEnd="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_weight="0.85"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_clinics"
                        tools:text="@string/lbl_clinics" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearTileMedicalRecord"
                    style="@style/groupStylegray_901"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageArticle"
                        android:layout_width="@dimen/_18pxh"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_article"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_article" />

                    <TextView
                        android:id="@+id/txtMedicalRecord"
                        style="@style/txtInterregular16_1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_marginEnd="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_weight="0.85"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_medical_record"
                        tools:text="@string/lbl_medical_record" />
                </LinearLayout>

            </LinearLayout>
            <LinearLayout android:id="@+id/linearColumntileTwo"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A700">

                <LinearLayout
                    android:id="@+id/linearTileFeedback"
                    style="@style/groupStylewhite_A700"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageFeedback"
                        android:layout_width="@dimen/_18pxh"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_feedback"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_feedback" />

                    <TextView
                        android:id="@+id/txtFeedback"
                        style="@style/txtInterregular16_2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_marginEnd="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_weight="0.85"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_feedback"
                        tools:text="@string/lbl_feedback" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearTileSignIn"
                    style="@style/groupStylewhite_A700"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageSignIn"
                        android:layout_width="@dimen/_18pxh"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_signin"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_signin" />

                    <TextView
                        android:id="@+id/txtSignIn"
                        style="@style/txtInterregular16_2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_marginEnd="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_weight="0.85"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_sign_in"
                        tools:text="@string/lbl_sign_in" />
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
